  28  125bi    6      9       12      5
  29  125m     4      8       10      4
  30  50bi     2      5       8       2
  31  50m      1      4       7       1;
  32   
  33  Parameters
  34      t(departamentos) 'trabajadores por departamento'
  35          / motor   17
  36            pintura 19
  37            montaje 8
  38            calidad 16/
  39   
  40      b(moto) 'beneficio de cada uno de los modelos'
  41          / 125bi 1070
  42            125m  920
  43            50bi  860
  44            50m   650/;
  45   
  46  Variables
  47      cantidad(moto) 'motos producidas de cada modelo'
  48      be 'beneficio obtenido'
  49   
  50  Positive Variables cantidad;
  51   
  52  Equations
  53      benef 'calculo del beneficio'
  54      manoObra(departamentos) 'limite en el uso de cada recurso';
  55   
  56  benef.. be =e= sum(moto, b(moto)*cantidad(moto));
  57  manoObra(departamentos).. sum(moto, h(moto, departamentos)*cantidad(moto)) =l= t(departamentos);
  58   
  59  Model aquaScooter / all /;
  60   
  61  OPTION LP=CPLEX;
  62   
  63  aquaScooter.OPTFILE=1;
  64   
  65  * porque maximizig be
  66  solve aquaScooter using lp maximizing be;


